<?php
 session_start();
 include_once ('processlog.php');

 if(session_unset() && session_destroy()){
     $logout_time=date('D M Y  H:i', time());
     $email=$_SESSION['email'];
     $logoutobject=[
         'logout_time'=>$logout_time,
         'logout_email'=>$email,
     ];
     file_put_contents("logtime/LogoutTime".$_SESSION['email'] . ".json",json_encode($logoutobject));
     echo "Data saved";
 session_unset();
 session_destroy();
 header("Location: index.php");
 }
?>
